echo $row['channel'];
echo "<td>";
}
?>

   <form name="form" method="POST" action="chatdup.php">
     <input value="<?php echo $tmp;?>" type="hidden" name="search">
     <input type="submit"  value="CHAT">
   </form>

<?php
}
   echo "</td></tr>";
